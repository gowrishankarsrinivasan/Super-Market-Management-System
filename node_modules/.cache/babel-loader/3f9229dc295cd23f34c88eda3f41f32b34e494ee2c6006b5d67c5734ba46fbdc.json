   </v-col>\r\n                  <v-col cols=\"12\">\r\n                    <v-text-field label=\"Phone\" v-model=\"profile.phone\" outlined dense></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\">\r\n                    <v-textarea label=\"Address\" v-model=\"profile.address\" outlined dense></v-textarea>\r\n                  </v-col>\r\n                  <v-col cols=\"12\">\r\n                    <v-text-field label=\"Date of Birth\" v-model=\"profile.dob\" outlined dense></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\">\r\n                    <v-text-field label=\"PAN Card Full Name\" v-model=\"profile.panFullName\" outlined dense></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\">\r\n                    <v-text-field label=\"PAN Card Number\" v-model=\"profile.panNumber\" outlined dense></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" class=\"text-center\">\r\n                    <v-avatar size=\"150\" @click=\"triggerPanCardFileInput\" class=\"pan-card-avatar\">\r\n                      <img :src=\"panCardImage || 'https://via.placeholder.com/150'\" alt=\"PAN Card Image\" class=\"pan-card-img\">\r\n                    </v-avatar>\r\n                    <input\r\n                      ref=\"panCardFileInput\"\r\n                      type=\"file\"\r\n                      accept=\"image/*\"\r\n                      @change=\"onPanCardFileChange\"\r\n                      style=\"display: none;\"\r\n                    />\r\n                    <v-btn class=\"mt-2\" small outlined @click=\"triggerPanCardFileInput\">Upload PAN Card</v-btn>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-col>\r\n            </v-row>\r\n            <v-divider class=\"my-4\"></v-divider>\r\n            <v-row>\r\n              <v-col cols=\"12\">\r\n                <v-btn block color=\"primary\" @click=\"openEditDialog\">Edit Profile</v-btn>\r\n                <v-btn block color=\"secondary\" outlined>Order History</v-btn>\r\n              </v-col>\r\n            </v-row>\r\n          </v-card>\r\n        </v-col>\r\n      </v-row>\r\n  \r\n      <!-- Profile Edit Dialog -->\r\n      <v-dialog v-model=\"editDialog\" max-width=\"600px\">\r\n        <v-card>\r\n          <v-card-title>Edit Profile</v-card-title>\r\n          <v-card-text>\r\n            <v-form>\r\n              <v-text-field label=\"Name\" v-model=\"editProfile.name\"></v-text-field>\r\n              <v-select\r\n                v-model=\"editProfile.gender\"\r\n                :items=\"genders\"\r\n                label=\"Gender\"\r\n              ></v-select>\r\n              <v-text-field label=\"Email\" v-model=\"editProfile.email\"></v-text-field>\r\n              <v-text-field label=\"Phone\" v-model=\"editProfile.phone\"></v-text-field>\r\n              <v-textarea label=\"Address\" v-model=\"editProfile.address\"></v-textarea>\r\n              <v-text-field label=\"Date of Birth\" v-model=\"editProfile.dob\"></v-text-field>\r\n              <v-text-field label=\"PAN Card Full Name\" v-model=\"editProfile.panFullName\"></v-text-field>\r\n              <v-text-field label=\"PAN Card Number\" v-model=\"editProfile.panNumber\"></v-text-field>\r\n            </v-form>\r\n          </v-card-text>\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"primary\" @click=\"saveProfile\">Save</v-btn>\r\n            <v-btn text @click=\"closeEditDialog\">Cancel</v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-dialog>\r\n    </v-container>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        profileImage: null,\r\n        panCardImage: null,\r\n        editDialog: false,\r\n        genders: ['Male', 'Female', 'Other'],\r\n        profile: {\r\n          name: \"John Doe\",\r\n          gender: \"Male\",\r\n          email: \"john.doe@example.com\",\r\n          phone: \"+1 234 567 890\",\r\n          address: \"123 Main St, Anytown, USA\",\r\n          dob: \"01/01/1990\",\r\n          panFullName: \"John Doe\",\r\n          panNumber: \"ABCDE1234F\"\r\n        },\r\n        editProfile: {}\r\n      };\r\n    },\r\n    methods: {\r\n      triggerFileInput() {\r\n        this.$refs.fileInput.click();\r\n      },\r\n      onFileChange(event) {\r\n        const file = event.target.files[0];\r\n        if (file) {\r\n          const reader = new FileReader();\r\n          reader.onload = (e) => {\r\n            this.profileImage = e.target.result;\r\n          };\r\n          reader.readAsDataURL(file);\r\n        }\r\n      },\r\n      triggerPanCardFileInput() {\r\n        this.$refs.panCardFileInput.click();\r\n      },\r\n      onPanCardFileChange(event) {\r\n        const file = event.target.files[0];\r\n        if (file) {\r\n          const reader = new FileReader();\r\n          reader.onload = (e) => {\r\n            this.panCardImage = e.target.result;\r\n          };\r\n          reader.readAsDataURL(file);\r\n        }\r\n      },\r\n      openEditDialog() {\r\n        // Initialize editProfile with current profile data\r\n        this.editProfile = { ...this.profile };\r\n        this.editDialog = true;\r\n      },\r\n      closeEditDialog() {\r\n        this.editDialog = false;\r\n      },\r\n      saveProfile() {\r\n        // Update the profile data with the edited values\r\n        this.profile = { ...this.editProfile };\r\n        this.closeEditDialog();\r\n        alert('Profile saved successfully!');\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .profile-avatar, .pan-card-avatar {\r\n    cursor: pointer;\r\n    border: 2px solid #E0E0E0;\r\n    border-radius: 50%;\r\n    overflow: hidden;\r\n  }\r\n  .avatar-img {\r\n    object-fit: cover;\r\n    width: 100%;\r\n    height: 100%;\r\n  }\r\n  .pan-card-img {\r\n   