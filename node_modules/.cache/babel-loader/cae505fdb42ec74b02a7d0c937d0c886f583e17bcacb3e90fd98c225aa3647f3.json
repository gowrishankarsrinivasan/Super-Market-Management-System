{"ast":null,"code":"import { mapGetters, mapActions } from \"vuex\";\nexport default {\n  name: \"ShoppingCart\",\n  data() {\n    return {\n      items: [{\n        title: 'My Profile'\n      }, {\n        title: 'Orders'\n      }, {\n        title: 'Click Me'\n      }, {\n        title: 'Click Me 2'\n      }]\n    };\n  },\n  computed: {\n    ...mapGetters([\"cartItems\"]),\n    totalPrice() {\n      return this.cartItems.reduce((acc, item) => acc + item.price, 0);\n    }\n  },\n  methods: {\n    formatCurrency(value) {\n      return `₹${value.toFixed(2)}`;\n    },\n    ...mapActions(['removeFromCart']),\n    removeItem(item) {\n      this.removeFromCart(item);\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","mapActions","name","data","items","title","computed","totalPrice","cartItems","reduce","acc","item","price","methods","formatCurrency","value","toFixed","removeItem","removeFromCart"],"sources":["D:\\E-commerce\\FrontEnd\\my-vue3-project\\src\\components\\DisplayCartItems.vue"],"sourcesContent":["<template>\r\n    <v-container>\r\n        <v-app-bar>\r\n            <v-app-bar-nav-icon class=\"d-sm-none\" @click.stop=\"drawer = !drawer\"></v-app-bar-nav-icon>\r\n\r\n            <v-toolbar-title>My Store</v-toolbar-title>\r\n\r\n            <v-spacer></v-spacer>\r\n            <div class=\"text-center\">\r\n                <v-menu open-on-hover>\r\n                    <template v-slot:activator=\"{ props }\">\r\n                        <v-btn color=\"primary\" v-bind=\"props\">\r\n                            Dropdown\r\n                        </v-btn>\r\n                    </template>\r\n\r\n                    <v-list>\r\n                        <v-list-item v-for=\"(item, index) in items\" :key=\"index\">\r\n                            <v-list-item-title>{{ item.title }}</v-list-item-title>\r\n                        </v-list-item>\r\n                    </v-list>\r\n                </v-menu>\r\n            </div>\r\n\r\n            <!-- Only show on medium and larger screens -->\r\n            <router-link to=\"/home\">\r\n                <v-btn text class=\"d-none d-md-inline-flex\">Home</v-btn>\r\n            </router-link>\r\n            <router-link to=\"/shop\">\r\n                <v-btn text class=\"d-none d-md-inline-flex\">Shop</v-btn>\r\n            </router-link>\r\n            <router-link to=\"/blog\">\r\n                <v-btn text class=\"d-none d-md-inline-flex\">Blog</v-btn>\r\n            </router-link>\r\n            <router-link to=\"/cart\">\r\n                <v-btn text class=\"d-none d-md-inline-flex\">Cart</v-btn>\r\n            </router-link>\r\n\r\n\r\n\r\n            <!-- Menu icon for smaller screens -->\r\n            <v-btn icon=\"mdi-dots-vertical\" class=\"d-md-none\" @click.stop=\"drawer = !drawer\"></v-btn>\r\n\r\n            <v-navigation-drawer v-model=\"drawer\" :location=\"$vuetify.display.mobile ? 'bottom' : undefined\" temporary>\r\n                <v-list>\r\n                    <v-list-item v-for=\"item in Navitems\" :key=\"item.value\" :to=\"{ path: '/' + item.value }\" router>\r\n                        <v-list-item-title>{{ item.title }}</v-list-item-title>\r\n                    </v-list-item>\r\n                </v-list>\r\n            </v-navigation-drawer>\r\n        </v-app-bar>\r\n        <v-row>\r\n            <!-- Left Section: Cart Items -->\r\n            <v-col cols=\"8\">\r\n                <v-card outlined>\r\n                    <v-card-title>From Saved Addresses</v-card-title>\r\n                    <v-divider></v-divider>\r\n\r\n                    <!-- Cart Items -->\r\n                    <v-card-subtitle v-for=\"(item, index) in cartItems\" :key=\"index\" class=\"pb-5 pt-5\">\r\n                        <v-row no-gutters align=\"center\">\r\n                            <v-col cols=\"2\">\r\n                                <v-img :src=\"item.image\" max-width=\"100\" max-height=\"100\"></v-img>\r\n                            </v-col>\r\n                            <v-col cols=\"6\">\r\n                                <div>{{ item.name }}</div>\r\n                                <div>{{ item.category }}</div>\r\n                                <div class=\"green--text\">Available</div>\r\n                            </v-col>\r\n                            <v-col cols=\"4\" class=\"text-right\">\r\n                                <div>{{ formatCurrency(item.price) }}</div>\r\n                                <!-- <div>{{ item.unit }}</div> -->\r\n                                <v-btn text>Save for Later</v-btn>\r\n                                <v-btn text @click=\"removeItem(item)\">Remove</v-btn>\r\n                            </v-col>\r\n                        </v-row>\r\n                        <v-divider></v-divider>\r\n                    </v-card-subtitle>\r\n                </v-card>\r\n            </v-col>\r\n\r\n            <!-- Right Section: Price Details -->\r\n            <v-col cols=\"4\">\r\n                <v-card outlined>\r\n                    <v-card-title>Price Details</v-card-title>\r\n                    <v-divider></v-divider>\r\n                    <v-card-text>\r\n                        <v-row>\r\n                            <v-col cols=\"6\">Total Items</v-col>\r\n                            <v-col cols=\"6\" class=\"text-right\">{{ cartItems.length }}</v-col>\r\n                        </v-row>\r\n                        <v-row>\r\n                            <v-col cols=\"6\">Total Amount</v-col>\r\n                            <v-col cols=\"6\" class=\"text-right\">{{ formatCurrency(totalPrice) }}</v-col>\r\n                        </v-row>\r\n                    </v-card-text>\r\n                    <v-card-actions>\r\n                        <v-btn color=\"orange\" block class=\"custom-hover-btn\">PLACE ORDER</v-btn>\r\n                    </v-card-actions>\r\n                </v-card>\r\n            </v-col>\r\n        </v-row>\r\n    </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters, mapActions } from \"vuex\";\r\n\r\nexport default {\r\n    name: \"ShoppingCart\",\r\n    data() {\r\n        return {\r\n            items: [\r\n                { title: 'My Profile' },\r\n                { title: 'Orders' },\r\n                { title: 'Click Me' },\r\n                { title: 'Click Me 2' },\r\n            ],\r\n        }\r\n    },\r\n    computed: {\r\n        ...mapGetters([\"cartItems\"]),\r\n\r\n        totalPrice() {\r\n            return this.cartItems.reduce((acc, item) => acc + item.price, 0);\r\n        }\r\n    },\r\n    methods: {\r\n        formatCurrency(value) {\r\n            return `₹${value.toFixed(2)}`;\r\n        },\r\n        ...mapActions(['removeFromCart']),\r\n\r\n        removeItem(item) {\r\n            this.removeFromCart(item);\r\n        }\r\n\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.text-right {\r\n    text-align: right;\r\n}\r\n\r\n.red--text {\r\n    color: red;\r\n}\r\n\r\n.custom-hover-btn:hover {\r\n    background-color: #FFA726 !important;\r\n    /* Change to your desired hover color */\r\n    color: white !important;\r\n    /* Optional: Change text color on hover */\r\n}\r\n</style>\r\n"],"mappings":"AA0GA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAE7C,eAAe;EACXC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,CACH;QAAEC,KAAK,EAAE;MAAa,CAAC,EACvB;QAAEA,KAAK,EAAE;MAAS,CAAC,EACnB;QAAEA,KAAK,EAAE;MAAW,CAAC,EACrB;QAAEA,KAAK,EAAE;MAAa,CAAC;IAE/B;EACJ,CAAC;EACDC,QAAQ,EAAE;IACN,GAAGN,UAAU,CAAC,CAAC,WAAW,CAAC,CAAC;IAE5BO,UAAUA,CAAA,EAAG;MACT,OAAO,IAAI,CAACC,SAAS,CAACC,MAAM,CAAC,CAACC,GAAG,EAAEC,IAAI,KAAKD,GAAE,GAAIC,IAAI,CAACC,KAAK,EAAE,CAAC,CAAC;IACpE;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,cAAcA,CAACC,KAAK,EAAE;MAClB,OAAO,IAAIA,KAAK,CAACC,OAAO,CAAC,CAAC,CAAC,EAAE;IACjC,CAAC;IACD,GAAGf,UAAU,CAAC,CAAC,gBAAgB,CAAC,CAAC;IAEjCgB,UAAUA,CAACN,IAAI,EAAE;MACb,IAAI,CAACO,cAAc,CAACP,IAAI,CAAC;IAC7B;EAEJ;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependenci