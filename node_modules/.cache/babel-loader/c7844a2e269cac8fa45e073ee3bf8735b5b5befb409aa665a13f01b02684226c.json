{"ast":null,"code":"import { mapGetters } from \"vuex\";\nexport default {\n  name: \"ShoppingCart\",\n  computed: {\n    ...mapGetters([\"cartItems\"]),\n    totalPrice() {\n      return this.cartItems.reduce((acc, item) => acc + item.price, 0);\n    }\n  },\n  methods: {\n    formatCurrency(value) {\n      return `₹${value.toFixed(2)}`;\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","name","computed","totalPrice","cartItems","reduce","acc","item","price","methods","formatCurrency","value","toFixed"],"sources":["D:\\E-commerce\\FrontEnd\\my-vue3-project\\src\\components\\DisplayCartItems.vue"],"sourcesContent":["<template>\r\n    <v-container>\r\n      <v-row>\r\n        <!-- Left Section: Cart Items -->\r\n        <v-col cols=\"8\">\r\n          <v-card outlined>\r\n            <v-card-title>From Saved Addresses</v-card-title>\r\n            <v-divider></v-divider>\r\n  \r\n            <!-- Cart Items -->\r\n            <v-card-subtitle v-for=\"(item, index) in cartItems\" :key=\"index\">\r\n              <v-row no-gutters align=\"center\">\r\n                <v-col cols=\"2\">\r\n                  <v-img :src=\"item.image\" max-width=\"100\" max-height=\"100\"></v-img>\r\n                </v-col>\r\n                <v-col cols=\"6\">\r\n                  <div>{{ item.name }}</div>\r\n                  <div>{{ item.category }}</div>\r\n                  <div class=\"red--text\">Out Of Stock</div>\r\n                </v-col>\r\n                <v-col cols=\"4\" class=\"text-right\">\r\n                  <div>{{ formatCurrency(item.price) }}</div>\r\n                  <div>{{ item.unit }}</div>\r\n                  <v-btn text>Save for Later</v-btn>\r\n                  <v-btn text>Remove</v-btn>\r\n                </v-col>\r\n              </v-row>\r\n              <v-divider></v-divider>\r\n            </v-card-subtitle>\r\n          </v-card>\r\n        </v-col>\r\n  \r\n        <!-- Right Section: Price Details -->\r\n        <v-col cols=\"4\">\r\n          <v-card outlined>\r\n            <v-card-title>Price Details</v-card-title>\r\n            <v-divider></v-divider>\r\n            <v-card-text>\r\n              <v-row>\r\n                <v-col cols=\"6\">Total Items</v-col>\r\n                <v-col cols=\"6\" class=\"text-right\">{{ cartItems.length }}</v-col>\r\n              </v-row>\r\n              <v-row>\r\n                <v-col cols=\"6\">Total Amount</v-col>\r\n                <v-col cols=\"6\" class=\"text-right\">{{ formatCurrency(totalPrice) }}</v-col>\r\n              </v-row>\r\n            </v-card-text>\r\n            <v-card-actions>\r\n              <v-btn color=\"orange\" block>PLACE ORDER</v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </template>\r\n  \r\n  <script>\r\n  import { mapGetters } from \"vuex\";\r\n  \r\n  export default {\r\n    name: \"ShoppingCart\",\r\n    computed: {\r\n      ...mapGetters([\"cartItems\"]),\r\n      \r\n      totalPrice() {\r\n        return this.cartItems.reduce((acc, item) => acc + item.price, 0);\r\n      }\r\n    },\r\n    methods: {\r\n      formatCurrency(value) {\r\n        return `₹${value.toFixed(2)}`;\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .text-right {\r\n    text-align: right;\r\n  }\r\n  .red--text {\r\n    color: red;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAyDE,SAASA,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,QAAQ,EAAE;IACR,GAAGF,UAAU,CAAC,CAAC,WAAW,CAAC,CAAC;IAE5BG,UAAUA,CAAA,EAAG;MACX,OAAO,IAAI,CAACC,SAAS,CAACC,MAAM,CAAC,CAACC,GAAG,EAAEC,IAAI,KAAKD,GAAE,GAAIC,IAAI,CAACC,KAAK,EAAE,CAAC,CAAC;IAClE;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAACC,KAAK,EAAE;MACpB,OAAO,IAAIA,KAAK,CAACC,OAAO,CAAC,CAAC,CAAC,EAAE;IAC/B;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}