{"ast":null,"code":"import { mapGetters, mapActions } from \"vuex\";\nexport default {\n  name: \"ShoppingCart\",\n  data() {\n    return {\n      drawer: false,\n      profileMenu: false,\n      Navitems: [{\n        title: 'Home',\n        value: 'home'\n      }, {\n        title: 'Shop',\n        value: 'shop'\n      }, {\n        title: 'Blog',\n        value: 'blog'\n      }, {\n        title: 'Cart',\n        value: 'cart'\n      }]\n    };\n  },\n  computed: {\n    ...mapGetters([\"cartItems\"]),\n    totalPrice() {\n      return this.cartItems.reduce((acc, item) => acc + item.price, 0);\n    }\n  },\n  methods: {\n    formatCurrency(value) {\n      return `₹${value.toFixed(2)}`;\n    },\n    ...mapActions(['removeFromCart']),\n    removeItem(item) {\n      this.removeFromCart(item);\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","mapActions","name","data","drawer","profileMenu","Navitems","title","value","computed","totalPrice","cartItems","reduce","acc","item","price","methods","formatCurrency","toFixed","removeItem","removeFromCart"],"sources":["D:\\E-commerce\\FrontEnd\\my-vue3-project\\src\\components\\DisplayCartItems.vue"],"sourcesContent":["<template>\r\n    <v-container>\r\n      <v-app-bar>\r\n        <v-app-bar-nav-icon class=\"d-sm-none\" @click.stop=\"drawer = !drawer\"></v-app-bar-nav-icon>\r\n  \r\n        <v-toolbar-title>My Store</v-toolbar-title>\r\n  \r\n        <v-spacer></v-spacer>\r\n  \r\n        <!-- Only show on medium and larger screens -->\r\n        <router-link to=\"/home\">\r\n          <v-btn text class=\"d-none d-md-inline-flex\">Home</v-btn>\r\n        </router-link>\r\n        <router-link to=\"/shop\">\r\n          <v-btn text class=\"d-none d-md-inline-flex\">Shop</v-btn>\r\n        </router-link>\r\n        <router-link to=\"/blog\">\r\n          <v-btn text class=\"d-none d-md-inline-flex\">Blog</v-btn>\r\n        </router-link>\r\n        <router-link to=\"/cart\">\r\n          <v-btn text class=\"d-none d-md-inline-flex\">Cart</v-btn>\r\n        </router-link>\r\n  \r\n        <!-- Menu icon for smaller screens -->\r\n        <v-btn icon=\"mdi-dots-vertical\" class=\"d-md-none\" @click.stop=\"drawer = !drawer\"></v-btn>\r\n  \r\n        <!-- Profile Menu with Hover Effect -->\r\n        <v-menu\r\n          v-model=\"profileMenu\"\r\n          open-on-hover\r\n          offset-y\r\n          transition=\"slide-x-reverse-transition\"\r\n        >\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-btn\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n              icon\r\n            >\r\n              <v-avatar>G</v-avatar>\r\n            </v-btn>\r\n          </template>\r\n          <v-list>\r\n            <v-list-item>\r\n              <v-list-item-title>Profile</v-list-item-title>\r\n            </v-list-item>\r\n            <v-list-item>\r\n              <v-list-item-title>Orders</v-list-item-title>\r\n            </v-list-item>\r\n            <v-list-item>\r\n              <v-list-item-title>Wishlist</v-list-item-title>\r\n            </v-list-item>\r\n            <v-list-item>\r\n              <v-list-item-title>Logout</v-list-item-title>\r\n            </v-list-item>\r\n          </v-list>\r\n        </v-menu>\r\n  \r\n        <v-navigation-drawer v-model=\"drawer\" :location=\"$vuetify.display.mobile ? 'bottom' : undefined\" temporary>\r\n          <v-list>\r\n            <v-list-item v-for=\"item in Navitems\" :key=\"item.value\" :to=\"{ path: '/' + item.value }\" router>\r\n              <v-list-item-title>{{ item.title }}</v-list-item-title>\r\n            </v-list-item>\r\n          </v-list>\r\n        </v-navigation-drawer>\r\n      </v-app-bar>\r\n  \r\n      <v-row>\r\n        <!-- Left Section: Cart Items -->\r\n        <v-col cols=\"8\">\r\n          <v-card outlined>\r\n            <v-card-title>From Saved Addresses</v-card-title>\r\n            <v-divider></v-divider>\r\n  \r\n            <!-- Cart Items -->\r\n            <v-card-subtitle v-for=\"(item, index) in cartItems\" :key=\"index\" class=\"pb-5 pt-5\">\r\n              <v-row no-gutters align=\"center\">\r\n                <v-col cols=\"2\">\r\n                  <v-img :src=\"item.image\" max-width=\"100\" max-height=\"100\"></v-img>\r\n                </v-col>\r\n                <v-col cols=\"6\">\r\n                  <div>{{ item.name }}</div>\r\n                  <div>{{ item.category }}</div>\r\n                  <div class=\"green--text\">Available</div>\r\n                </v-col>\r\n                <v-col cols=\"4\" class=\"text-right\">\r\n                  <div>{{ formatCurrency(item.price) }}</div>\r\n                  <!-- <div>{{ item.unit }}</div> -->\r\n                  <v-btn text>Save for Later</v-btn>\r\n                  <v-btn text @click=\"removeItem(item)\">Remove</v-btn>\r\n                </v-col>\r\n              </v-row>\r\n              <v-divider></v-divider>\r\n            </v-card-subtitle>\r\n          </v-card>\r\n        </v-col>\r\n  \r\n        <!-- Right Section: Price Details -->\r\n        <v-col cols=\"4\">\r\n          <v-card outlined>\r\n            <v-card-title>Price Details</v-card-title>\r\n            <v-divider></v-divider>\r\n            <v-card-text>\r\n              <v-row>\r\n                <v-col cols=\"6\">Total Items</v-col>\r\n                <v-col cols=\"6\" class=\"text-right\">{{ cartItems.length }}</v-col>\r\n              </v-row>\r\n              <v-row>\r\n                <v-col cols=\"6\">Total Amount</v-col>\r\n                <v-col cols=\"6\" class=\"text-right\">{{ formatCurrency(totalPrice) }}</v-col>\r\n              </v-row>\r\n            </v-card-text>\r\n            <v-card-actions>\r\n              <v-btn color=\"orange\" block class=\"custom-hover-btn\">PLACE ORDER</v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </template>\r\n  \r\n  <script>\r\n  import { mapGetters, mapActions } from \"vuex\";\r\n  \r\n  export default {\r\n    name: \"ShoppingCart\",\r\n    data() {\r\n      return {\r\n        drawer: false,\r\n        profileMenu: false,\r\n        Navitems: [\r\n          { title: 'Home', value: 'home' },\r\n          { title: 'Shop', value: 'shop' },\r\n          { title: 'Blog', value: 'blog' },\r\n          { title: 'Cart', value: 'cart' },\r\n        ],\r\n      };\r\n    },\r\n    computed: {\r\n      ...mapGetters([\"cartItems\"]),\r\n  \r\n      totalPrice() {\r\n        return this.cartItems.reduce((acc, item) => acc + item.price, 0);\r\n      }\r\n    },\r\n    methods: {\r\n      formatCurrency(value) {\r\n        return `₹${value.toFixed(2)}`;\r\n      },\r\n      ...mapActions(['removeFromCart']),\r\n  \r\n      removeItem(item) {\r\n        this.removeFromCart(item);\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .text-right {\r\n    text-align: right;\r\n  }\r\n  \r\n  .red--text {\r\n    color: red;\r\n  }\r\n  \r\n  .custom-hover-btn:hover {\r\n    background-color: #FFA726 !important;\r\n    /* Change to your desired hover color */\r\n    color: white !important;\r\n    /* Optional: Change text color on hover */\r\n  }\r\n  </style>\r\n  "],"mappings":"AA0HE,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAE7C,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,KAAK;MACbC,WAAW,EAAE,KAAK;MAClBC,QAAQ,EAAE,CACR;QAAEC,KAAK,EAAE,MAAM;QAAEC,KAAK,EAAE;MAAO,CAAC,EAChC;QAAED,KAAK,EAAE,MAAM;QAAEC,KAAK,EAAE;MAAO,CAAC,EAChC;QAAED,KAAK,EAAE,MAAM;QAAEC,KAAK,EAAE;MAAO,CAAC,EAChC;QAAED,KAAK,EAAE,MAAM;QAAEC,KAAK,EAAE;MAAO,CAAC;IAEpC,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGT,UAAU,CAAC,CAAC,WAAW,CAAC,CAAC;IAE5BU,UAAUA,CAAA,EAAG;MACX,OAAO,IAAI,CAACC,SAAS,CAACC,MAAM,CAAC,CAACC,GAAG,EAAEC,IAAI,KAAKD,GAAE,GAAIC,IAAI,CAACC,KAAK,EAAE,CAAC,CAAC;IAClE;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAACT,KAAK,EAAE;MACpB,OAAO,IAAIA,KAAK,CAACU,OAAO,CAAC,CAAC,CAAC,EAAE;IAC/B,CAAC;IACD,GAAGjB,UAAU,